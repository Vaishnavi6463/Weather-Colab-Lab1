#hardcoded example for quadratic equation: ax^2 + bx + c = 0
a = float(input("Enetr a: "))
b = float(input("Enetr b: "))
c = float(input("Enetr c: "))

D = b**2 - 4*a*c

if D < 0:
     print("No real roots")
else:
    root1 = (-b + D**0.5) / (2*a)
    root2 = (-b - D**0.5) / (2*a)
    print(f"Roots are : {root1}, {root2}")
